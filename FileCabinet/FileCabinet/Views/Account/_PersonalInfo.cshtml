@model FileCabinet.Models.UserProfile

<div class="row">
    <div class="col-sm-6 toppad">
        <div class="panel panel-info">
            <div class="panel-heading">
                <h3 class="panel-title">Profile</h3>
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-md-3 col-lg-3 " align="center"> <img alt="User Pic" src="data:image/png;base64," class=" img-circle"> </div>
                    <div class=" col-md-9 col-lg-9 ">
                        <table class="table table-user-information">
                            <tbody>
                                <tr>
                                    <td>@Html.LabelFor(model => model.Username)</td>
                                    <td>@Html.DisplayFor(model => model.Username)</td>
                                </tr>
                                <tr>
                                    <td>@Html.LabelFor(model => model.Email)</td>
                                    <td>@Html.DisplayFor(model => model.Email)</td>
                                </tr>
                                <tr>
                                    <td>@Html.LabelFor(model => model.DateOfBirth)</td>
                                    <td>@Html.DisplayFor(model => model.DateOfBirth)</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <div class="panel-info">
                <a class='btn btn-info btn-xs pull-right' href="/Account/Edit"><span class="glyphicon glyphicon-edit"></span> Edit profile</a>
            </div>
        </div>
    </div>
</div>
