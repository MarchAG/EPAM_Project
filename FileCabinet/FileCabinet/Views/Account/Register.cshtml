@model FileCabinet.Models.Register

@{
    ViewBag.Title = "Register";
}

<h2>Register</h2>
<div>
    @using (Html.BeginForm())
    {
        @Html.ValidationSummary(true)
        @Html.AntiForgeryToken()
        <div class="container">
            <div class="row">
                <div class="col-sm-6 col-md-4 col-md-offset-4">
                    <div>
                        <div  class="tab-content">
                            <div class="tab-pane active" id="login">
                                <div align="center">
                                    <img class="profile-img" src="data:image/png;base64,"
                                         alt="">
                                </div>
                                <div style="margin-left:10%">
                                    <div>
                                        @Html.LabelFor(m => m.Username)
                                        @Html.TextBoxFor(m => m.Username, new { @class = "form-control" })
                                        @Html.ValidationMessageFor(m => m.Username)
                                    </div>
                                    <div>
                                        @Html.LabelFor(m => m.EMail)
                                        @Html.TextBoxFor(m => m.EMail, new { @class = "form-control" })
                                        @Html.ValidationMessageFor(m => m.EMail)
                                    </div>
                                    <div>
                                        @Html.LabelFor(m => m.Password)
                                        @Html.PasswordFor(m => m.Password, new { @class = "form-control" })
                                        @Html.ValidationMessageFor(m => m.Password)
                                    </div>
                                    <div>
                                        @Html.LabelFor(m => m.ConfPassword)
                                        @Html.PasswordFor(m => m.ConfPassword, new { @class = "form-control" })
                                        @Html.ValidationMessageFor(m => m.ConfPassword)
                                    </div>
                                </div>
                                <div align="center" style="margin-top:2%">
                                    <input type="submit" class="btn btn-lg btn-success " value="Sign In" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    }
</div>

